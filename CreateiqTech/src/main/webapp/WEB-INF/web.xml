<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Login</servlet-name>
  	<display-name>Login</display-name>
  	<description></description>
  	<servlet-class>com.loginCreateiqController.Login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Welcome</servlet-name>
  	<display-name>Welcome</display-name>
  	<description></description>
  	<servlet-class>com.loginCreateiqController.Welcome</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>PageRedirecting</servlet-name>
  	<display-name>PageRedirecting</display-name>
  	<description></description>
  	<servlet-class>com.PageRedirect.PageRedirecting</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Welcome</servlet-name>
  	<url-pattern>/Welcome</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>PageRedirecting</servlet-name>
  	<url-pattern>/Search</url-pattern>
  </servlet-mapping>
 
  
  	
</web-app>

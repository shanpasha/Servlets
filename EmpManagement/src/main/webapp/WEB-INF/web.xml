<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>EmpController</servlet-name>
  	<display-name>EmpController</display-name>
  	<description></description>
  	<servlet-class>com.controller.EmpController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ControllerWithPost</servlet-name>
  	<display-name>ControllerWithPost</display-name>
  	<description></description>
  	<servlet-class>com.controller.ControllerWithPost</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>EmpController</servlet-name>
  	<url-pattern>/Emm</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ControllerWithPost</servlet-name>
  	<url-pattern>/Post</url-pattern>
  </servlet-mapping>
</web-app>

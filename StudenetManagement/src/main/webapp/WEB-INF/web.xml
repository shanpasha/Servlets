<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>GetStudentController</servlet-name>
  	<display-name>GetStudentController</display-name>
  	<description></description>
  	<servlet-class>com.Controller.GetStudentController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddStudents</servlet-name>
  	<display-name>AddStudents</display-name>
  	<description></description>
  	<servlet-class>com.Controller.AddStudents</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteStudent</servlet-name>
  	<display-name>DeleteStudent</display-name>
  	<description></description>
  	<servlet-class>com.Controller.DeleteStudent</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateStudent</servlet-name>
  	<display-name>UpdateStudent</display-name>
  	<description></description>
  	<servlet-class>com.Controller.UpdateStudent</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginPage</servlet-name>
  	<display-name>LoginPage</display-name>
  	<description></description>
  	<servlet-class>com.Controller.LoginPage</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GetStudentController</servlet-name>
  	<url-pattern>/getStudents</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddStudents</servlet-name>
  	<url-pattern>/addStudents</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteStudent</servlet-name>
  	<url-pattern>/deleteStudent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateStudent</servlet-name>
  	<url-pattern>/updateStudent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginPage</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
</web-app>
